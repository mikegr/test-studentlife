qx.Theme.define("studentlife.theme.Appearance",
{
  extend : qx.theme.modern.Appearance,

  appearances :
  {

    "label" :
    {
      style : function(states)
      {
        return {
          textColor : "#FF0000"
        };
      }
    },

  }
});